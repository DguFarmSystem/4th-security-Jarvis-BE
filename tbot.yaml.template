version: v2
onboarding:
  token: "${JOIN_TOKEN}"
  join_method: token
storage:
  type: directory
  path: /var/lib/teleport/bot
outputs:
  - type: identity
    destination:
      type: directory
      path: /opt/machine-id
    ssh_config: "on"
    allow_reissue: true

  - type: identity
    roles: [teleport-event-handler] 
    destination:
      type: directory
      path: /shared-certs/event-handler-identity
proxy_server: openswdev.duckdns.org:3080
debug: true